{
    "version": 2,
    "builds": [
      {
        "src": "front", 
        "use": "@vercel/static-build",
        "config": {
          "outputDirectory": "front-build" 
        }
      },
      {
        "src": "back",
        "use": "@vercel/python", 
        "config": {
          "maxLambdaSize": "1024mb" 
        }
      }
    ],
    "routes": [
      {
        "src": "/projeto/front/(.*)",
        "dest": "/front-build/$1" 
      },
      {
        "src": "/projeto/back/(.*)", 
        "dest": "/back/$1" 
      },
      {
        "src": "/(.*)",
        "dest": "/front-build/index.html" 
      }
    ]
  }
  